        def dfs(root1, root2):
            if not root1 and not root2:
                return None
            elif not root1:
                return root2
            elif not root2:
                return root1
            else:
                root1.val += root2.val
                root1.left = dfs(root1.left, root2.left)
                root1.right = dfs(root1.right, root2.right)
                return root1

        return dfs(root1, root2)
                
        